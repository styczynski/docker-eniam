chętny	sentp(żeby)
chętny	prepnp(do,dop)
głodny	np(dop)
gotowy	infp(_)
należny	np(cel)
niezbędny	np(cel)+sentp(żeby)
oburzony	sentp(że)
pełny	np(dop)
pewien	sentp(że)
pewien	np(dop)
pewny	sentp(że)
pewny	np(dop)
potrzebny	np(cel)+sentp(żeby)
przeciwny	np(cel)
przeciwny	sentp(cel,żeby)
przekonany	sentp(że)
szczęśliwy	sentp(że)
wierny	np(cel)
wierny	sentp(cel,że)
wściekły	sentp(że)
zaangażowany	prepnp(w,bier)
zadowolony	sentp(że)
zadowolony	sentp(z,dop,że)
zadowolony	prepnp(z,dop)
zgodny	sentp(że)
